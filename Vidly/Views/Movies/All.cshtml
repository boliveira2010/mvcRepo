@model IEnumerable<Vidly.Models.Movie>
@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Count() movies in store</h2>


<table>
    <tr>
        <td>@Html.DisplayNameFor(m => m.First().Id)</td>
        <td>@Html.DisplayNameFor(m => m.First().Name)</td>
    </tr>
    
    @foreach (var m in Model)
    {
        <tr>

            <td width="30px">@m.Id)</td>
            <td>@m.Name</td>
        </tr>
    }
</table>
